{
  "name": "XHR Capture",
  "description": "Capture XHR requests and save it with multiple format",
  "version": "0.1.0",
  "manifest_version": 3,
  "permissions": ["webRequest", "downloads"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "favicon.png",
      "32": "favicon.png",
      "48": "favicon.png",
      "128": "favicon.png"
    }
  },
  "icons": {
    "16": "favicon.png",
    "32": "favicon.png",
    "48": "favicon.png",
    "128": "favicon.png"
  },
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  }
}
